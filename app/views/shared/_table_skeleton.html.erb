<tbody data-time-table-target="skeleton">
  <% 5.times do %>
    <tr class="animate-pulse odd:bg-white even:bg-gray-50 dark:odd:bg-gray-900 dark:even:bg-gray-800">
      <% 4.times do %>
        <td class="px-6 py-4">
          <div class="h-4 bg-gray-200 rounded dark:bg-gray-700 w-2/3"></div>
        </td>
      <% end %>
    </tr>
  <% end %>
</tbody>
